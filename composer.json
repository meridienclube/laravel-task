{
    "name": "meridienclube/laravel-task",
    "version": "dev-master",
    "description": "Laravel Task Pack",
    "keywords": [
        "task",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Rafael Zingano",
            "email": "confrariaweb@gmail.com"
        }
    ],
    "require": {
        "meridienclube/laravel-comment": "dev-master",
        "meridienclube/laravel-datetimepicker": "dev-master",
        "meridienclube/laravel-entrust": "dev-master",
        "meridienclube/laravel-historic": "dev-master",
        "meridienclube/laravel-modal": "dev-master",
        "meridienclube/laravel-option": "dev-master",
        "meridienclube/laravel-select2": "dev-master",
        "meridienclube/laravel-user": "dev-master",
        "meridienclube/laravel-vendor": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "ConfrariaWeb\\Task\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConfrariaWeb\\Task\\Providers\\TaskAuthServiceProvider",
                "ConfrariaWeb\\Task\\Providers\\TaskEventServiceProvider",
                "ConfrariaWeb\\Task\\Providers\\TaskServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
